{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "virtual-web-live",
    "version": "1.0.0"
  },
  "tauri": {
    "updater": {
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDk1RkQ3RkVDRDYxNzREOEMKUldTTVRSZlc3SC85bFR2NE8zbENnZS82MnN2ODRRcEtOR1dNUS8zVWNyeFRPL0JQNjk1THlVNEIK",
      "endpoints": [
        "https://releases.myapp.com/{{windows}}/{{arch}}/{{current_version}}"
      ]
    },
    "allowlist": {
      "all": true,
      "http": {
        "scope": [
          "http://**",
          "https://**"
        ]
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "net.fcios.ws",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "windows": {
        "certificateThumbprint": null,
        "wix": {
          "language": "zh-CN"
        },
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "昊轩数字人",
        "width": 1200,
        "height": 800,
        "minWidth": 1200,
        "minHeight": 800,
        "label": "main"
      }
    ]
  }
}